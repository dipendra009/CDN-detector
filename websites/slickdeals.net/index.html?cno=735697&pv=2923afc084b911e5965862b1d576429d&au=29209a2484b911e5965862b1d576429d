<script>
/* 24/7 Chat start */
if (shc.features.chat247) {
  shc.chat = {
    pageDetails: {
     pxpath: shc.env === 'PROD' ? 'PROD' : 'STAGE',
     international: !!shc.features.international,
     language: 'US',
     isPR: !!(shc.features.searsPR || shc.features.kmartPR),
     pageName: $('body').attr('id') || 'none',
     productVertical: typeof omVrt !== 'undefined' ? omVrt : '',
     productCategory: typeof omCat !== 'undefined' ? omCat : '',
     productSubCategory: typeof omSubCat !== 'undefined' ? omSubCat : ''
   },
   EVENT: {pageLoad:'pageload', click: 'click'},
   SCOPE: {page: 'page', visitor: 'visitor'},
   send: function(scope, key, data, event) {
     if (window._tfsq === undefined) {
       window._tfsq = [];
     }
     _tfsq.push([scope, key, data, event]);
   }
  };

  if ($('body').attr('id') === 'product') {
    $(document).on(shcWidgets.EVENTS.chatDataReady, function(e, data) {
      if (data.data) {
        $.extend(shc.chat.pageDetails, data.data);
      }
      var ch = shc.chat;
      //defer setting this b/c it's dependant on header getting all profile info
      ch.pageDetails.loginFlag = $('#profileArea.loggedIn').length > 0;
      ch.send(ch.SCOPE.page, 'pageDetails', ch.pageDetails, ch.EVENT.pageLoad);
    });
  }
  else {
    $(window).load(function() {
      var ch = shc.chat;
      //defer setting this b/c it's dependant on header getting all profile info
      ch.pageDetails.loginFlag = $('#profileArea.loggedIn').length > 0;
      ch.send(ch.SCOPE.page, 'pageDetails', ch.pageDetails, ch.EVENT.pageLoad);
    });
  }
}
/* 24/7 Chat end */
</script>



<script>
shc.doAdSense1 = function(pageId) {
  var p1, p2, pubId = shc.storeName;
  
  switch (pageId) {
   case 'product': pubId += '-pdp'; break;
   case 'search-page':
    pubId += (bt.pageType === 'search' ? '-search' : '-browse');
    //kill the iframe b/c paging is not a refresh & subsequent ad calls won't happen if iframe is already there
    $('#gnf_footer_GADS').html('');
    break;
  }
  
  p1 = {
    pubId: pubId,
    adtest: shc.env === 'PROD' ? 'off' : 'on',
    query:  (pageId === 'product' ? bt.productName : pubId.indexOf('-search')>0 ? bt.keyWord :
          (bt.levels || bt.categoryName || bt.verticalName)).replaceAll('_',' '),
    channel: (bt.verticalName || bt.categoryName || 'misc').replace(/[\s&\s\,]/g, '_').replace(/_+/g, '_').toLowerCase(),
    adPage: $.query.load(location.search).get('pageNum') || 1
    };
  p2 = {
    container: 'gnf_footer_GADS', width: '600px',
      colorLocation: '#333', colorAnnotation: '#666', colorAttribution: '#333', colorBackground: '#FFF',
    colorBorder: '#d0d0d0', colorDomainLink: '#333', colorText: '#333', colorTitleLink: '#333',
    fontSizeAnnotation: 13, fontSizeDescription: 13, fontSizeDomainLink: 13, fontSizeTitle: 15, fontSizeLocation: 13,
    siteLinks: true, lineHeightTitle: 20, lineHeightDescription: 14, lineHeightDomainLink: 14, titleBold: true,
    verticalSpacing: 2, linkTarget: '_blank', longerHeadlines: true,
    number: 6, sellerRatings: true, detailedAttribution: true
  };

  $('#gnf_footer .GADS_wrap').show();
  console.log('Google AdSense1 called called for pubId=' + pubId);
  _googCsa('ads', p1, p2);
};

shc.doAdSense2 = function(pageId)  {
  var p1, p2, pubId = 'vert-pla-sears' + (shc.storeName==='kmart'?'-kmart': '');

  switch (pageId) {
    case 'product': pubId += '-pdp'; break;
    case 'search-page':
      pubId += (bt.pageType === 'search' ? '-srp' : '-cat');
        //kill the iframe b/c paging is not a refresh & subsequent ad calls won't happen if iframe is already there
        $('#gnf_footer_GADS').html('');
      break;
  }

  p1 = {
    pubId: pubId,
    query: (pageId === 'product' ? bt.productName : pubId.indexOf('-srp')>0 ? bt.keyWord :
     (bt.levels || bt.categoryName || bt.verticalName)).replaceAll('_',' '),
    channel: (bt.verticalName || bt.categoryName || 'misc').replace(/[\s&\s\,]/g, '_').replace(/_+/g, '_').toLowerCase(),
    adsafe: 'high',
    adtest: shc.env === 'PROD' ? 'off' : 'on',
    hl: 'en'
  };

  p2 = {
    container: 'gnf_footer_GADS',
    width: '600px',
    height: '600px',
    adLoadedCallback: function(el, loaded) {
      if (!loaded) {
        shc.doAdSense1(pageId); //fall back to text ads if no visual ads returned
      }
    }
  };

  $('#gnf_footer .GADS_wrap').show();
  console.log('Google AdSense2 called for pubId=' + pubId);
  _googCsa('plas', p1, p2);
};

$(function() {
  //include Adsense only only on search/pdp
  var pageId = $.q('body').attr('id');

  if (['sears', 'kmart'].indexOf(shc.storeName) >= 0 &&
    (pageId === 'search-page' || pageId === 'product') && $('#gnf_footer_GADS').length) {
    $(document).on(shcWidgets.EVENTS.signalDataReady, function() {      
      if (!shc.AdSenseJS) {
        FED.Util.addJsFile('//www.google.com/adsense/search/async-ads.js')
          .done(function() {
            shc.AdSenseJS = true;
            shc['doAdSense' + (shc.storeName === 'sears' || pageId === 'product' ? '1' : '2')](pageId);
          });
      }
      else {
        //already loaded file - just call
        shc['doAdSense' + (shc.storeName === 'sears' || pageId === 'product' ? '1' : '2')](pageId);
      }
    });
  }
});

$(window).on('load', function() {
   //Delete WCS session cookies to prevent them being carried over in IRP requests
   $.each(document.cookie.split(/; */), function()  {
	var cookieName = this.split('=')[0];			
	if(!!cookieName && (/(^WC_)|(^TS)|(^wcid_)|(c_i)|(_igur)|(_igh)|(_ig)|(SSID)|(SSOD)|(SSPV)|(SSRT)/.test(cookieName)) && cookieName !== 'WC_PERSISTENT'){
		console.log("Deleting " + cookieName + " cookie");
		document.cookie = cookieName + '=null; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
	}
   });
});
</script>




</div><script type="text/javascript">

	window.bt = {"levelIds":"",
	"levels":"",
	"verticalId":"",
	"verticalName":"",
	"categoryID":"",
	"categoryName":"",
	"subCategoryID":"",
	"subCategoryName":"",
	"itemNumber":"",
	"beanType":"",
	"itemPrice":"N/A",
	"itemDiscount":"N/A",
	"productDescription":"",
	"oasSitePage":"sears.com/home/index/vertical"};
	(function() {
		var catArray = shc.catArray;
		var catIdArray =  shc.catIdArray;
		if (catArray.length > 0) {
			var i = 0, l = Math.min(3, catArray.length);
			for (i = 0; i < l; i++) {
				switch (i) {
					case 0:
						bt.verticalName = catArray[i];
						bt.levels = bt.verticalName;
						bt.verticalId = catIdArray[i];
						bt.levelIds = bt.verticalId;
						break;
					case 1:
						bt.categoryName = catArray[i];
						bt.levels += '_'+bt.categoryName;
						bt.categoryID = catIdArray[i];
						if(bt.categoryID !== ""){
						bt.levelIds += '_'+bt.categoryID;
						}
						break;
					case 2:
						bt.subCategoryName = catArray[i];
						bt.levels += '_'+bt.subCategoryName;
						bt.subCategoryID = catIdArray[i];
						if(bt.subCategoryID !== ""){
						bt.levelIds += '_'+bt.subCategoryID;
						}
				}
			}
		}
	})();
</script>


<script>

	jQuery('#livenowtab .channel-filter').unbind('click');
	jQuery('#livenowtab .channel-filter').each(function(i){
		jQuery(this).click(function(e){
			e.preventDefault();

			//filter channels
			filterChan = jQuery(this).attr('id');

			if(filterChan != "channel-all"){
				window.location.hash = processHashArg('channel', filterChan);
			}else{
				window.location.hash = unprocessHashArg('channel');
			}

			GlobalFilter.setChannelDropdown(filterChan);
			GlobalFilter.filterChannel(filterChan, true);
			GlobalFilter.cleanUpSections();

		});
	});
</script>

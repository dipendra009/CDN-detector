<script>e3const.launchPlayer = null;
		if(country =='australia'){
			window.location = 'http://espn3.com.au';
		}else if(country =='new zealand'){
			window.location = 'http://espn3.co.nz';
		}
	
		var mobileRedirect = false;
		var rbSrc = "null";
		var ua = navigator.userAgent;
		var qs = "?watchsrc=true";

		if(ua.match(/iPad/i) || ua.match(/iP(od|hone)/i)){
			qs = qs + "&device=ios";
			mobileRedirect = true;
		}else if(ua.match(/Android/i)){
			qs = qs + "&device=android";
			mobileRedirect = true;
		}
		else if(ua.match(/Blackberry/i)){
			qs = qs + "&device=blackberry";
			device="blackberry";
			mobileRedirect = true;
		}
		else if(ua.match(/IEMobile/i)){
			qs = qs + "&device=windows_phone";
			device="windows_phone";
			mobileRedirect = true;
		}
		else if(ua.match(/Kindle/i)){
			qs = qs + "&device=kindle";
			device="kindle";
			mobileRedirect = true;
		}
		
		if(mobileRedirect && rbSrc != "true" && g_pageType != "watchespnfaq"){
			
				qs = qs +"&";
			
			window.location = "http://espn.go.com/watchespn/roadblock"+qs;	
		}
	
		if(window.location.hash != null && window.location.hash != ''){
			var redirectUrl = window.location.href;
			var hasParams = false;
			if(redirectUrl.indexOf("/_/") != -1){
				hasParams = true;
				redirectUrl = redirectUrl.substring(redirectUrl.indexOf("/_/")+3, redirectUrl.indexOf("#"));
			}

			var redirectArgs = '';

			var validArgs = ["type", "sport", "channel", "league", "upcoming-days", "days", "search", "startDate"];

			var redirectUrlAry = redirectUrl.split("/");
			var urlHashAry = window.location.hash.replace("#", "").split("/");

			/***
			*
			*	FOR ALL PARAMETERS THAT AREN'T ALREADY IN THE PARAM LIST
			*
			*/
			if(urlHashAry.length > 0){
				for(i = 0; i < urlHashAry.length; i++){
					if(jQuery.inArray(urlHashAry[i], validArgs) != -1){
						var hasHashArg = false;
						for(j = 0; j < redirectUrlAry.length; j++){
							if(redirectUrlAry[j] == urlHashAry[i]){
								hasHashArg = true;
							}
						}
						if(!hasHashArg){
							redirectArgs = redirectArgs + urlHashAry[i] + '/' + urlHashAry[i+1].replace('livenow', 'live') + '/';
						}
					}
				}
			}

			/**
			*
			*	NOW WE WANT TO REPLACE ALL PARAMS WITH VALUES THAT ARE ON THE HASH IF THEY EXIST
			*
			*/
			if(redirectUrlAry.length > 0 && hasParams){

				for(i = 0; i < redirectUrlAry.length; i++){
					if(jQuery.inArray(redirectUrlAry[i], validArgs) != -1){
						var hasHashArg = false;
						for(j = 0; j < urlHashAry.length; j++){
							if(redirectUrlAry[i] == urlHashAry[j]){
								redirectArgs = redirectArgs + redirectUrlAry[i] + '/' + urlHashAry[j+1].replace('livenow', 'live')	+ '/';
								hasHashArg = true;
							}
						}
						if(!hasHashArg){
							redirectArgs = redirectArgs + redirectUrlAry[i] + '/' + redirectUrlAry[i+1].replace('livenow', 'live') + '/';
						}
					}
				}
			}else{
				redirectArgs = window.location.hash.replace("#", "");
			}
			window.location = e3const.p_WATCHESPN + '/index/_/' + redirectArgs;
		}
	e3const.source = "";</script>
	<script language="javascript">
	var ad_site = 'espn.us.com.watchespn';
	var ad_zone = 'index';
	var ad_kvps = 'pgtyp=watchespnindex;sp=espn;';
	var ad_root = 'watchespn';
	var ad_swid = '';
	var ad_counter = 1;
	var ad_ord = Math.floor(Math.random()*9999999999);
	var ad_seg = '';

	ad_swid = jQuery.cookie('SWID');
	if(typeof espn.core.ad_segments == 'function') {
		ad_seg = espn.core.ad_segments();
		if (ad_seg != null && ad_seg != '') {
			ad_kvps = ad_kvps + ad_seg
		}
	}

	// the u
	var ad_u = 'u=pgtyp=watchespnindex|sp=espn';
	var ad_seg_u = ad_seg;
	if (ad_swid != null && ad_swid != '' && ad_swid != 'null') {
		if (ad_swid.length > 54) {
			ad_swid = ad_swid.substring(0,54);
		}
		ad_newu = 'u=swid='+ad_swid+'|';
		ad_u = ad_u.replace('u=',ad_newu);
	}
	if (ad_seg_u != null && ad_seg_u != '' && ad_seg_u != 'null') {
		while(ad_seg_u.indexOf(';')!=-1) {
			ad_seg_u = ad_seg_u.replace(';','|');
		}
		ad_u = ad_u + '|' + ad_seg_u;
	}
	ad_u = ad_u + ';';
	if (ad_u.length > 255) {
		ad_u = ad_u.substring(0,254);
	}
	</script>
	

<script>

			jQuery('#livenowtab .category-filter').unbind('click');
			jQuery('#livenowtab .category-filter').each(function(i){
				jQuery(this).click(function(e){
					e.preventDefault();

					//filter categories
					var filterCat = jQuery(this).attr('id');
					var fitlerCatCanon = jQuery(this).parent().attr('id');

					if(filterCat != "category-all"){
						window.location.hash = processHashArg('sport', fitlerCatCanon);
					}else{
						window.location.hash = unprocessHashArg('sport');
					}

					GlobalFilter.setCategoryDropdown(filterCat);
					GlobalFilter.filterCategory(filterCat, true);
					GlobalFilter.cleanUpSections();
				});
			});
		</script>
</div>

<script>
(function($){
	$('.2016-positionRecSelect').change(function(){
		var position = $(this).val()
		if(position == "null"){
			$('#2016-content table.topRecModWPos tbody').hide()
			$('#2016-content table.topRecModWPos tbody.overallTopRec').show()
			$('#2016-content table.topRecModWPos thead tr').replaceWith('<tr><th width="52%">Player</th><th width="16%">Grade</th><th width="16%">Pos</th><th width="16%">School</th></tr>')
		} else {
			$('#2016-content table.topRecModWPos tbody').hide()
			$('#2016-content table.topRecModWPos tbody.'+position).show()
			$('#2016-content table.topRecModWPos thead tr').replaceWith('<tr><th width="16%">Rank</th><th width="52%">Player</th><th width="16%">Grade</th><th width="16%">School</th></tr>')
		}
	});
})(jQuery)
</script></div>

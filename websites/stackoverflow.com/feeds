        <link rel="alternate" href="http://stackoverflow.com/questions/33573854/object-reference-not-set-to-an-instance-of-an-object-html-dropdownlistfor-whil" />
        <published>2015-11-06T19:03:16Z</published>
        <updated>2015-11-06T19:03:16Z</updated>
        <summary type="html">
            

            &lt;p&gt;I am trying to make a form and complete the form, It contains a dropdownlist. But I am facing &lt;/p&gt;

&lt;blockquote&gt;
  &lt;p&gt;Object reference not set to an instance of an object.&lt;/p&gt;
&lt;/blockquote&gt;

&lt;p&gt;while I am trying to submit the form. Help me..&lt;/p&gt;

&lt;h2&gt;Model Class&lt;/h2&gt;

&lt;pre&gt;&lt;code&gt; public class CustomerRequestModel
{
    [Required]
    [DataType(DataType.Text)]
    public string Address { get; set; }

    [Required]
    [DataType(DataType.Date)]
    public DateTime Date { get; set; }

    [Required]
    [DataType(DataType.Time)]
    public DateTime Time { get; set; }


    public SelectList Service { get; set; }

    [HiddenInput]
    public int SelectedValue { get; set; }
}
&lt;/code&gt;&lt;/pre&gt;

&lt;h2&gt;HttpGet Method in Controller&lt;/h2&gt;

&lt;pre&gt;&lt;code&gt; [HttpGet]
 public ActionResult ServiceRequest()
 {
     CustomerRequestModel Model = new CustomerRequestModel();
     Model.Service = new SelectList(db.Services, &quot;Id&quot;, &quot;Name&quot;);
  }
&lt;/code&gt;&lt;/pre&gt;

&lt;h2&gt;HttpPost Method in Controller&lt;/h2&gt;

&lt;pre&gt;&lt;code&gt;  public ActionResult ServiceRequest(CustomerRequestModel model)
    {
        if (ModelState.IsValid)
        {
             // I&#39;ll do whatever I like
        }
        return View();
    }
&lt;/code&gt;&lt;/pre&gt;

&lt;h2&gt;View&lt;/h2&gt;

&lt;pre&gt;&lt;code&gt; @Html.DropDownListFor(m =&amp;gt; m.SelectedValue, Model.Service, &quot;Select a service&quot;, new { @class = &quot;form-control&quot;})
&lt;/code&gt;&lt;/pre&gt;

&lt;p&gt;&lt;a href=&quot;http://i.stack.imgur.com/NluPt.png&quot; rel=&quot;nofollow&quot;&gt;This is the problem I am facing. I don&#39;t know how to solve this&lt;/a&gt;&lt;/p&gt;

